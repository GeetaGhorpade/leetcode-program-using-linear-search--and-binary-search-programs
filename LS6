#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> arr = {4, 2, 7, 1, 9};
    int maxVal = arr[0];
    
    // Linear search to update the maximum value
    for (int i = 1; i < arr.size(); i++) {
        if (arr[i] > maxVal) {
            maxVal = arr[i];
        }
    }
    
    cout << "Maximum element is: " << maxVal << endl;
    // Expected output: Maximum element is: 9
    return 0;
}
