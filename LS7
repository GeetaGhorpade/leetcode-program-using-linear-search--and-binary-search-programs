#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> arr = {4, 2, 7, 1, 9};
    int minVal = arr[0];
    
    // Compare each element to find the minimum
    for (int i = 1; i < arr.size(); i++) {
        if (arr[i] < minVal) {
            minVal = arr[i];
        }
    }
    
    cout << "Minimum element is: " << minVal << endl;
    // Expected output: Minimum element is: 1
    return 0;
}
